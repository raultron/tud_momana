<?xml version="1.0" encoding="utf-8"?>
<launch>

  <include file="$(find tud_momana)/launch/ar_sys_multi_boards_external.launch" />

  <arg name="joy_dev" default="/dev/input/js0" />

  <node pkg="joy" type="joy_node" name="joy_node">
    <param name="dev" value="$(arg joy_dev)" />
    <param name="deadzone" value="0.1" />
    <param name="autorepeat_rate" value="20" />
  </node>

  <node pkg="tud_momana" name="joy_control_momana" type="joy_control_momana"/>

  <node pkg="ardrone_velocity" name="pid_control" type="pid_control" output="screen" />  

  <node pkg="tud_coop_uv" name="merge" type="merge" output="screen" />

  <node pkg="tud_coop_uv" name="tracking" type="tracking" output="screen" />

  <node pkg="tud_coop_uv" name="define_target" type="define_target" output="screen" />

  <node pkg="tud_momana" name="momana" type="momana" output="screen"/>

  <node pkg="tud_momana" name="momana_odom" type="momana_odom" output="screen"/>

  <node pkg="tf2_ros" type="static_transform_publisher" name="c3po_base_link_tf" args="0 0 0 0 0 0 1 c3po_marker_top c3po_base_link" />

  <node pkg="tf2_ros" type="static_transform_publisher" name="r2d2_base_link_tf" args="0 0 0 0 0 0 1 r2d2_marker_top r2d2_base_link" />

</launch>
